    <span class="token comment">//加载登录页面</span>
  <span class="token keyword">public</span> <span class="token keyword">function</span> <span class="token function">login</span><span class="token punctuation">(</span>HomeLoginRequest <span class="token variable">$request</span><span class="token punctuation">)</span><span class="token punctuation">{</span>

        <span class="token comment">//判断用户是邮箱登录还是电话号码登录</span>

       <span class="token variable">$data</span><span class="token punctuation">[</span><span class="token single-quoted-string string">\'password\'</span><span class="token punctuation">]</span><span class="token operator">=</span><span class="token variable">$request</span><span class="token punctuation">[</span><span class="token single-quoted-string string">\'password\'</span><span class="token punctuation">]</span><span class="token punctuation">;</span>

